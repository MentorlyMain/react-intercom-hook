{
  "name": "@reclaim-ai/react-intercom-hook",
  "version": "2.0.0",
  "description": "React hook for Intercom.io",
  "repository": "git://github.com/reclaim-ai/react-intercom-hook.git",
  "keywords": [
    "react",
    "react-hooks",
    "intercom",
    "intercom.io"
  ],
  "author": "Ian White <ian@reclaimai.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reclaim-ai/react-intercom-hook/issues"
  },
  "homepage": "https://github.com/reclaim-ai/react-intercom-hook#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "start": "npm run storybook",
    "build": "npm run lint && npm run clean && npm run build:library && npm run build:storybook",
    "build:library": "rimraf lib && tsc && npm run copy-assets",
    "build:storybook": "build-storybook -s public",
    "build:watch": "rimraf lib && tsc --watch",
    "test": "jest --maxWorkers 2",
    "test:watch": "jest --watch",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "lint:types": "tsc --noEmit",
    "storybook": "start-storybook -p 6006",
    "copy-assets": "cpx \"./src/**/*.{css,woff2,svg,png}\" ./lib",
    "build-storybook": "build-storybook",
    "prepublishOnly": "npm run build",
    "clean": "rimraf lib storybook-static"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@storybook/addon-actions": "^5.3.10",
    "@storybook/addon-docs": "^5.3.10",
    "@storybook/addon-links": "^5.3.10",
    "@storybook/addons": "^5.3.10",
    "@storybook/preset-typescript": "^1.2.0",
    "@storybook/react": "^5.3.10",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.3.2",
    "@testing-library/react-hooks": "^3.2.1",
    "@testing-library/user-event": "^8.1.0",
    "@types/jest": "^25.1.1",
    "@types/jsdom": "^12.2.4",
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "cpx": "^1.5.0",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jest-dom": "^2.0.0",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "fork-ts-checker-webpack-plugin": "^4.0.3",
    "jest": "^25.1.0",
    "jsdom": "^16.1.0",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^25.2.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4"
  },
  "dependencies": {}
}
